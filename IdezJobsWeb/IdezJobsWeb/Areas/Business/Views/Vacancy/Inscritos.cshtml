@model IEnumerable<IdezJobsWeb.Models.Profile>
@{
	ViewBag.Title = "Inscritos";
	Layout = "~/Areas/Business/Views/Shared/_Layout.cshtml";
}
<h2>
	Inscritos</h2>
<p>
	@Html.ActionLink("Create New", "Create")
</p>
<table>
	<tr>
		<th>
			Code
		</th>
		<th>
			FirstName
		</th>
		<th>
			LastName
		</th>
		<th>
			PictureUrl
		</th>
		<th>
			PublicUrl
		</th>
		<th>
			Headline
		</th>
		<th>
			Industry
		</th>
		<th>
		</th>
	</tr>
	@foreach (var item in Model)
 {

		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.Code)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.FirstName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.LastName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.PictureUrl)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.PublicUrl)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Headline)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Industry)
			</td>
			<td>
				@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
				@Html.ActionLink("Details", "Details", new { id = item.Id }) |
				@Html.ActionLink("Delete", "Delete", new { id = item.Id })
			</td>
		</tr>	

 }
</table>
